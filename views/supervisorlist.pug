doctype html
html(lang="en")
  head
    link(rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css")
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title SUPERVISOR REGISTRATION FORM 
    div.row.justify-content-center
        div.col-md-8
            div.card-body.border   
                header.card-header
                    h4.card-title Registered Supervisors
                table.table
                    thead.thead-dark
                        tr 
                            th First Name
                            th Last Name
                            th Sex
                            th Date of Birth
                            th Place of Residence
                            th Phone Number
                            th Clubs
                        tbody
                            //- iteration method 
                            each supervisor in supervisors
                                tr
                                    td #{supervisor.firstName}
                                    td #{supervisor.lastName}
                                    td #{supervisor.sex}
                                    td #{supervisor.dateOfBirth}
                                    td #{supervisor.residence}
                                    td #{supervisor.phoneNumber}
                                    td #{supervisor.club}